Section:NewButton("Gamepass giver", "Lol poor(but srsly who the fuck gonna buy a ballon for 80)", function()
    local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")


StarterGui:SetCore("SendNotification", {
	Title = "IMPORTANT!";
	Text = "THIS SCRIPT IS FE, SO SOME OF THE TOOLS WILL NOT WORK. YOU ALSO WONT GET THE TOOLS IF SOMEBODY DOESNT ALREADY OWN THEM";
	Duration = 15;
})


StarterGui:SetCore("SendNotification", {
	Title = "hey sisters";
	Text = "script made by placehold0r";
	Icon = "rbxassetid://108841980072551";
	Duration = 5;
})

local LocalPlayer = Players.LocalPlayer
local backpack = LocalPlayer:FindFirstChild("Backpack")

if backpack then
	for i, player in Players:GetPlayers() do
		if player ~= LocalPlayer then
			local playerBackpack = player:FindFirstChild("Backpack")
			if playerBackpack then
				for j, tool in playerBackpack:GetChildren() do
					local clone = tool:Clone()
					clone.Parent = backpack
				end
			end
		end
	end
end
end)
